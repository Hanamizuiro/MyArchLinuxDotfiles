    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "text" : "Shutdown",
    "keybind" : "s"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "text" : "Reboot",
    "keybind" : "r"
}
{
    "label" : "logout",
    "action" : "loginctl kill-session $XDG_SESSION_ID",
    "text" : "Logout",
    "keybind" : "e"
}
{
    "label" : "lock",
    "action" : "$HOME/.config/hypr/scripts/LockScreen.sh",
    "text" : "Lock",
    "keybind" : "l"
}
{
    "label" : "sleep",
    "action" : "loginctl lock-session && systemctl suspend",,
    "text" : "Sleep",
    "keybind" : "u"
}
{
    "label" : "hibernate",
    "action" : "systemctl hibernate",
    "text" : "Hibernate",
    "keybind" : "h"
